.back-to-top {
	position: fixed; 
	cursor: pointer; 
	background: transparent url(./images/icon-backtotop.png) center no-repeat;
	z-index: 999;
	bottom: 20px;
	right: 20px;
	height: 50px;
	width: 50px; 	
	/*background-size: 30px 30px !important;*/
	border: 2px solid rgba(210,188,146, 1);
	border-radius: 40px;
	display: none;
	-webkit-transition: all 0.4s ease;
    -moz-transition: all 0.4s ease;
    -o-transition: all 0.4s ease;
    transition: all 0.4s ease;	
}

.back-to-top:hover {
	background-color: #2f2b30;
	border: 2px solid #2f2b30;
}

.back-to-top:before {
	color:#A98E60;
	display: block;
	line-height: 50px;
	font-size: 24px;
	vertical-align: middle;
	text-align: center;
	-webkit-transition: all 0.4s ease;
    -moz-transition: all 0.4s ease;
    -o-transition: all 0.4s ease;
    transition: all 0.4s ease;	
}

.back-to-top:hover:before {
	color: #FFFFFF;
}


